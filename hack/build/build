#!/usr/bin/env bash

hack/build/operator/build
hack/build/backup-operator/build
hack/build/restore-operator/build

VERSION=$(grep 'Version = ' version/version.go|cut -d '=' -f2|sed -e  's| ||g' -e 's|"||g' -e 's|+git||g')

docker build -f hack/build/Dockerfile -t us-east1-docker.pkg.dev/tks-gcr-pub/etcd-operator/etcd-operator:v0.10.2-tks .
docker push us-east1-docker.pkg.dev/tks-gcr-pub/etcd-operator/etcd-operator:v0.10.2-tks